         <iu id="org.springframework.tooling.bosh.ls.feature"/>
         <iu id="org.springframework.tooling.concourse.ls.feature"/>
         <icon
               image32="images/sts4-32.png">
         </icon>
         <overview
               summary="The Spring Tools 4 Add-On Extension Pack allows you to install the latest version of the Spring Tools 4 features as
			   an add-on into an existing STS 3.9.x installation. Please keep in mind that this add-on is not meant for production use. We recommend to migrate
			   to the full Spring Tools4 for Eclipse distribution instead as soon as possible.">
         </overview>
      </connectorDescriptor>

